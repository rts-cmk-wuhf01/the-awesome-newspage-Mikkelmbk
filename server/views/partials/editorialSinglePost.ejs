<!-- Single Post -->
                    <%if(typeof editorialSinglePostData != 'undefined'){  %>
                        <% editorialSinglePostData.forEach((editorialSingleData)=>{
                            let d = new Date(editorialSingleData.date) %>
                    <div class="single-blog-post style-2">
                        <div class="post-thumb">
                            <a href="#"><img src="/img/bg-img/<%=editorialSingleData.image%>" alt=""></a>
                        </div>
                        <div class="post-data">
                            <a href="#" class="post-title">
                                <h6><%=editorialSingleData.textContent%></h6>
                            </a>
                            <div class="post-meta">
                                <div class="post-date"><a href="#"><%=dateAndTime.format(d,' MMMM DD, YYYY')%></a></div>
                            </div>
                        </div>
                    </div>
                     <% }) %>
                        <% } %>
