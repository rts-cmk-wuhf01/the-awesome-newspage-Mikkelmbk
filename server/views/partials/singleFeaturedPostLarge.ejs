<div class="single-blog-post featured-post">
                                <div class="post-thumb">
                                    <a href="/single-article/<%=largeFeaturedPostData.article_id%>"><img src="/img/bg-img/<%=largeFeaturedPostData.image_name%>" alt=""></a>
                                </div>
                                <div class="post-data">
                                    <a href="#" class="post-catagory"><%=largeFeaturedPostData.category_title%></a>
                                    <a href="#" class="post-title">
                                        <h6><%=largeFeaturedPostData.article_title%></h6>
                                    </a>
                                    <div class="post-meta">
                                        <p class="post-author">By <a href="#"><%=largeFeaturedPostData.author_name%></a></p>
                                        <p class="post-excerp"><%=largeFeaturedPostData.article_content%></p>
                                        <!-- Post Like & Post Comment -->
                                        <div class="d-flex align-items-center">
                                            <a href="#" class="post-like"><img src="/img/core-img/like.png" alt="">
                                                <span><%=largeFeaturedPostData.article_comment_count%></span></a>
                                            <a href="#" class="post-comment"><img src="/img/core-img/chat.png" alt="">
                                                <span><%=largeFeaturedPostData.article_like_count%></span></a>
                                        </div>
                                    </div>
                                </div>
                            </div>