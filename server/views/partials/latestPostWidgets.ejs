<!-- Latest Posts Widget -->
<div class="latest-posts-widget mb-50">

    <%if(typeof latestPostWidgetData != 'undefined'){   %>
    <% latestPostWidgetData.forEach((postData)=>{
        let d = new Date(postData.dateTime); %>
        

    <!-- Single Featured Post -->
    <div class="single-blog-post small-featured-post d-flex">
        <div class="post-thumb">
            <a href="#"><img src="img/bg-img/<%= postData.image %>" alt=""></a>
        </div>
        <div class="post-data">
            <a href="#" class="post-catagory"><%= postData.title %></a>
            <div class="post-meta">
                <a href="#" class="post-title">
                    <h6><%= postData.textContent %></h6>
                </a>
                <p class="post-date"><span><%= d.getHours() %></span> | <span> <%= d.getDate() %>/<%= d.getMonth()+1 %></span></p>
            </div>
        </div>
    </div>

    <%  }) %>
    
    <% }%>