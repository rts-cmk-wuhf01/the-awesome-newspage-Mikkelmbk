<% if( typeof returnMessageData != 'undefined'){ %>
	<h4><%= returnMessageData %></h4>
<% } %>
<form method="post">
  <label>Titel</label>


  <input type="text" name="category_title" value="<%= typeof categoriesData != 'undefined' ? categoriesData.category_title : '' %>">



  <button>Gem</button>
</form>

<table>
  <thead>
    <tr>
      <th>Actions</th>
      <th>Id</th>
      <th>Titel</th>
    </tr>
  </thead>

  <% if(typeof categoriesData != 'undefined') { %>
  <tbody>
    <% categoriesData.forEach(category => { %>
    <tr>
      <td>
        <a href="/admin/categories/edit/<%= category.category_id %>">Ret</a>
        <a
          href="/admin/categories/delete/<%= category.category_id %>"
          onclick="return confirm('Er du sikker pÃ¥ du vil slette?')">Slet</a>
      </td>
      <td><%= category.category_id %></td>
      <td><%= category.category_title %></td>
    </tr>
    <% }) %>
  </tbody>
  <% } %>
</table>